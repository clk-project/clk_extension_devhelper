#!/bin/bash -eu

source "_clk.sh"

clk_usage () {
    cat<<EOF
$0

Show the listening ports
--
O:--filter:str:Filter the ports using this string
EOF
}

clk_help_handler "$@"

netstat -tupln | {
    if [ -n "${CLK___FILTER}" ]
    then
        grep -i "${CLK___FILTER}"
    else
        cat
    fi

}
